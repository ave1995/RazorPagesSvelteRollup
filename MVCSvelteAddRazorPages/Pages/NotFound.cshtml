@page
@using Microsoft.AspNetCore.Razor.Hosting;
@using MVCSvelteAddRazorPages.Pages;
@attribute [RazorCompiledItemMetadata("RouteTemplate", Urls.NotFoundUrl)]
@model NotFoundModel
@{
    ViewData["Title"] = "404 Not Found";
    Layout = "_Layout";
}

@section HeadSection {
    <link href="~/build/404.css" asp-append-version="true" rel="stylesheet" />
    <script defer src="~/build/404.js" asp-append-version="true"></script>
}